{
  "manifest_version": 3,
  "name": "Tabster",
  "version": "0.1",
  "description": "React based Chrome Extension for chrome tab mangement",
  "permissions": ["tabs", "activeTab"],
  "author": "Richwin",
  "short_name": "Tabster",
  "action": {
    "default_popup": "index.html",
    "default_title": "Tabster"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./content.js"]
    }
  ]
}
