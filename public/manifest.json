{
  "manifest_version": 3,
  "name": "React Chrome Extension 2",
  "version": "0.1",
  "description": "React based Chrome Extension using Google's Manifest v3",
  "permissions": ["tabs", "activeTab"],
  "author": "Richwin",
  "short_name": "React Extension 2",
  "action": {
    "default_popup": "index.html",
    "default_title": "React Chrome Extension 2"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./content.js"]
    }
  ]
}
